--회원
create table users(
    userId varchar2(50) primary key,
    userPw varchar2(50) not null,
    userName varchar2(50) not null,
    userPhone varchar2(20) not null,
    userEmail varchar2(50) not null , /*두개로 받아서 저장은 하나로 받기*/
    addrAll varchar2(700) ,
    regdate date default sysdate
);

--카카오
create table kakaoUser(
    kakaoEmail varchar2(50) not null , /*두개로 받아서 저장은 하나로 받기*/
    kakaoBirt varchar2(50) 
    
);
