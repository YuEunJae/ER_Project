CREATE TABLE KICKLIST 
(
    MARKNO NUMBER(10) PRIMARY KEY, 
    PCODE VARCHAR2(100) NOT NULL, 
    CCODE VARCHAR2(100) NOT NULL, 
    LOCATION_X NUMBER(10) NOT NULL, 
    LOCATION_Y NUMBER(10) NOT NULL, 
    ISRENTAL NUMBER(2),
    rentalDate date
);

CREATE SEQUENCE KICKLIST_SEQ INCREMENT BY 1 START WITH 1 NOCYCLE NOCACHE;